{
  "root": true,
  "parser": "espree",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:promise/recommended",
    "prettier"
  ],
  "plugins": ["@typescript-eslint", "prettier", "promise"],
  "rules": {
    "prettier/prettier": ["error",
      {
        "trailingComma": "es5"
      }]
  }
}
