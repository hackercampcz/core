        <img
          class="responsive" loading="lazy" alt="" width="500" height="281"
          src="{{ publicId | cloudinaryUrl(width=500, height=281, crop="fill", dpr="auto") }}"
          srcset="{{ publicId | cloudinaryUrl(width=500, height=281, crop="fill", dpr="1") }} 1x,
