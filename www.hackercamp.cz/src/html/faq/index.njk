        #e9444e 25.57%,
        #e62271 42.71%,
        #684997 78.9%,
        #3e7abc 100.02%
      );
      --hc-gradient-btn: linear-gradient(
        90deg,
        #ee771b -0.01%,
        #e9444e 11.28%,
        #e62271 24.31%,
        #684997 36.29%,
