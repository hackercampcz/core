{
  "typescript": {
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "markup": {
    "printWidth": 120,
    "closingBracketSameLine": true,
    "doctypeKeywordCase": "upper",
    "html.scriptIndent": true,
    "html.styleIndent": true,
    "html.normal.selfClosing": false,
    "html.void.selfClosing": false
  },
  "malva": {
    "hexColorLength": "short",
    "linebreakInPseudoParens": true
  },
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.90.1.wasm",
    "https://plugins.dprint.dev/json-0.19.2.wasm",
    "https://plugins.dprint.dev/markdown-0.16.4.wasm",
    "https://plugins.dprint.dev/toml-0.6.1.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.9.0.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.4.0.wasm"
  ]
}
